

NAME  = ["Cash", "Bank", "Equipment", "Accounts Receivable","Accounts Payable","Debt","Office Expenses","Project Expenses","Capital","Revenue"]
BALANCE  = [1000,5000,3000,8000,10000,7000,3000,5000,3500,4500]
TYPE  = ["Asset","Asset","Asset","Asset","Liability","Liability","Expense","Expense","Equity","Equity"]

10.times do |i|
  name = NAME[i]
  balance = BALANCE[i]
  account_type = TYPE[i]
  puts "#{name}, #{balance}, #{account_type}"
  Account.create(:name => name,:balance => balance, :account_type => account_type)
end



Transaction.create(account1_id: 1,account2_id: 9,dir1:"to",dir2:"to",amount:50000.0 , description: "first seed")
Transaction.create(account1_id: 2,account2_id: 10,dir1:"to",dir2:"to",amount:30000.0 , description: "second seed")
Transaction.create(account1_id: 3,account2_id: 4,dir1:"from",dir2:"to",amount:50000.0 , description: "third seed")
